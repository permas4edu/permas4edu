!DEFAULT SET TOPOVAR = NODAL ! not suitable for free education edition

NEW

INPUT
    READ PERMAS FILE = nurbs.dat
    READ PERMAS FILE = nurbs_to.dat
RETURN

TASK LOOPS = 100
EXEC
    ACT SIT = STEP_1
    STATIC
    ACT SIT = TOPO
    TOPO METHOD = ACP
EXPORT
    ITEM CLEAR
    ITEM EFRATIO
    GO PERMAS BINARY FILE = 'efratio_%.3(tloop)'
    ITEM CLEAR
    ITEM XYDATA TYPE = OHIS
    ITEM XYDATA TYPE = CHIS
    ITEM XYDATA TYPE = RHIS
    GO PERMAS BINARY FILE = 'nurbs_xy_%.3(tloop)'
TASK END
STOP
