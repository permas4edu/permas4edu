NEW

INPUT
    READ PERMAS FILE = xinyong.dat
RETURN

TASK
EXEC
    ACT SIT = STEP_1
    VIBRATION ANALYSIS MODES = 27
    GENERATE STRAIN ENERGY
    GENERATE KINETIC ENERGY
SELECT
    PARTSUM SETBIN = SUM_K STRAIN ENERGY SORT = NONE NAME = SE
    PARTSUM SETBIN = SUM_M KINETIC ENERGY SORT = NONE NAME = KE
EXPORT
    ITEM MODL
    GO PERMAS BINARY FILE = normal_modes
    ITEM CLEAR
    ITEM PARTSUM NAME = SE
    GO PERMAS ASCII FILE = strain_energy
    ITEM CLEAR
    ITEM PARTSUM NAME = KE
    GO PERMAS ASCII FILE = kinetic_energy
TASK END
STOP
